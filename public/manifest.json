{
  "name": "WebMidiMapper",
  "version": "0.0.1",
  "author": "Rikard Lindstrom",
  "manifest_version": 2,
  "description": "Map DOM elements to MIDI!?",
  "homepage_url": "https://rikard.io",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": ["https://*/*", "http://*/*", "tabs"]
}
